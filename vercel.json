{
  "buildCommand": "vite build --config vite.config.ts",
  "outputDirectory": "dist/public",
  "framework": null,
  "installCommand": "npm install",
  "functions": {
    "server/index.ts": {
      "runtime": "nodejs18.x"
    }
  },
  "rewrites": [
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}