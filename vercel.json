{
  "version": 2,
  "buildCommand": "vite build",
  "outputDirectory": "dist/public",
  "rewrites": [
    {
      "source": "/api/contact",
      "destination": "/api/contact"
    },
    {
      "source": "/api/apply", 
      "destination": "/api/apply"
    },
    {
      "source": "/(.*)",
      "destination": "/index.html"
    }
  ]
}